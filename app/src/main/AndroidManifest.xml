<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jingdong_demo">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fragment.homepager.view.WebViewActivity"/>
        <activity android:name=".fragment.classify.view.ListActivity"/>
        <activity android:name=".register.RegisterActivity"/>
        <activity android:name=".login.view.LoginActivity"/>
        <activity android:name=".fragment.classify.sousuo.ShowActivity"/>
        <activity android:name=".fragment.classify.view.ListDetailsActivity"/>
        <activity android:name=".fragment.classify.view.SouSuoActivity"/>
        <activity android:name=".fragment.find.LineActivity"/>
    </application>

</manifest>